
<form method="post">
    <table  border="1">
        <tr>
            <td>Name :</td>
            <td> <input type="text" name="name"> </td>
        </tr>
        <tr>
            <td>Email :</td>
            <td> <input type="email" name="email"> </td>
        </tr>
        <tr>
            <td>Password :</td>
            <td> <input type="password" name="password"> </td>
        </tr>
        <tr>
            <td></td>
            <td> <input type="submit"> </td>
        </tr>
        
    </table>
</form>

<table  border="1">
    <th>ID</th>
    <th>Name</th>
    <th>Email</th>
    <th>Password</th>
    <th>Delete</th>
    <th>Update</th>

    <% result.forEach(function(data) { %>
        <tr>
            <td><%=data.id%></td>
            <td><%=data.name%></td>
            <td><%=data.email%></td>
            <td><%=data.password%></td>
            <td><a href="/delete/<%= data.id %>">Delete</a></td>
            <td><a href="/update/<%= data.id %>">Update</a></td>
        </tr>
    <% }); %>
</table>